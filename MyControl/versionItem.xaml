<UserControl x:Class="CleanLauncher.MyControl.versionItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CleanLauncher.MyControl"
             mc:Ignorable="d" 
             xmlns:pu="clr-namespace:Panuon.UI.Silver;assembly=Panuon.UI.Silver"
             d:DesignHeight="50" d:DesignWidth="700">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Image Height="40" Width="40" Source="/Image/GrassBlock.png" />
            <TextBlock Height="15" Width="100" x:Name="version" Text="Version" FontSize="13" Margin="20,0,0,0" />
            <TextBlock Height="15" Width="100" x:Name="time" Text="Time" FontSize="13" Margin="20,0,0,0" />
            <TextBlock Height="15" Width="100" x:Name="type" Text="Type" FontSize="13" Margin="20,0,0,0" />
        </StackPanel>
    </Grid>
</UserControl>
